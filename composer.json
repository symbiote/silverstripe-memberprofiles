{
	"name": "symbiote/silverstripe-memberprofiles",
	"description": "Member registration and profile management",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"member",
		"profile",
		"registration"
	],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Symbiote",
		"homepage": "https://www.symbiote.com.au/"
	}],
	"require": {
		"silverstripe/cms": "^4.11"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.0",
		"phpunit/phpunit": "^9.5"
	},
	"scripts": {
		"phpcbf": "phpcbf -n src/Email/ src/Extensions/ src/Forms/ src/Model/ src/Pages/ tests/"
	},
	"suggest": {
		"symbiote/silverstripe-gridfieldextensions": "Allows drag-and-drop reordering of fields"
	},
	"extra": {
		"expose": [
			"client"
		],
		"branch-alias": {
			"dev-master": "4.0.x-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"Symbiote\\MemberProfiles\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Symbiote\\MemberProfiles\\Tests\\": "tests/"
		}
	},
	"replace": {
		"silverstripe/memberprofiles": "self.version",
		"ajshort/silverstripe-memberprofiles": "self.version",
		"silverstripe-australia/memberprofiles": "self.version"
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
